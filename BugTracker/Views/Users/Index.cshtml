@model IEnumerable<Users>

<h2>Users</h2>
<table class="table">
    <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>email</th>
        <th>Title</th>
        <th>Password</th>
    </tr>

    @foreach (var user in Model)
    {
<tr>
    <td><a href=/Users/ViewUser/@user.UserID>@user.UserID</td>
    <td>@user.FirstName</td>
    <td>@user.LastName</td>
    <td>@user.email</td>
    <td>@user.Title</td>
    <td>@user.Password</td>
</tr>

    }
</table>
